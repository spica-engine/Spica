{
  "$schema": "http://spica.internal/passport/policy",
  "_id": "ApiKeyFullAccess",
  "name": "Api Key Full Access",
  "description": "Full access to passport api service.",
  "statement": [
    {
      "effect": "allow",
      "action": "passport:apikey:*",
      "resource": "*",
      "service": "passport:apikey"
    }
  ]
}