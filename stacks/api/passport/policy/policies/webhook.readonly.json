{
    "$schema": "http://spica.internal/passport/policy",
    "_id": "WebhookReadOnlyAccess",
    "name": "Webhook Readonly Access",
    "description": "Read only access to webhook service.",
    "statement": [
      {
        "effect": "allow",
        "action": "webhook:index",
        "resource": "*",
        "service": "webhook"
      }
    ]
  }